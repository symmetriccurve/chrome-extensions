{
    "name": "PNR PLUGIN",
    "version": "0.0.1",
    "manifest_version": 2,
    "description": "Handy Dandy tool to autofill",
    "icons": { 
        "16": "heart.png",
        "48": "heart.png",
        "128": "heart.png" 
    },
    "background": {
        "scripts": [
            "background.js"
        ],
        "persistent": false
    },
    "browser_action": {
        "default_icon": {
            "19": "heart.png",
            "38": "heart.png"
        },
        "default_title": "React Chrome Extension"
    },
    "permissions": [
        "http://*/",
        "https://*/",
        "activeTab",
        "storage",
        "tabs"
    ]
}